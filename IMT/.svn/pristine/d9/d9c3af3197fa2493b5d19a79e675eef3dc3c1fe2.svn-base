package com.imt.sys.entity;

import java.io.Serializable;

public class PageLimitAll implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private int psize;// 每一页的记录数
	private int pcount;//总共多少个数据
	private int pindex;//当前页
	
	private int ptotalpages;//总页数

	public int getPsize() {
		return psize;
	}

	public void setPsize(int psize) {
		this.psize = psize;
	}

	public int getPcount() {
		return pcount;
	}

	public void setPcount(int pcount) {
		this.pcount = pcount;
	}

	public int getPindex() {
		return pindex;
	}

	public void setPindex(int pindex) {
		this.pindex = pindex;
	}

	public int getPtotalpages() {
		return ptotalpages;
	}

	public void setPtotalpages(int ptotalpages) {
		this.ptotalpages = ptotalpages;
	}
	
}
