<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 命名空间，使用别名 -->
	<typeAliases >
		<typeAlias alias="SysUser" type="com.imt.sys.entity.SysUserEntity" />
		<typeAlias alias="pageLimit" type="com.imt.sys.entity.PageLimit"/>
		<typeAlias alias="pLimit" type="com.imt.pat.entity.PageLimit"/>
		<typeAlias alias="patient" type="com.imt.pat.entity.patientEntity"/>
		<typeAlias alias="country" type="com.imt.pat.entity.Country"/>
		<typeAlias alias="disease" type="com.imt.pat.entity.Disease"/>
		<typeAlias alias="net" type="com.imt.pat.entity.Net"/>
		<typeAlias alias="dep" type="com.imt.sys.entity.DepartmentEntity"/>
		<typeAlias alias="job" type="com.imt.sys.entity.JobEntity"/>
	</typeAliases>
	<!-- 映射map mapper下的xml文件 -->
	<mappers>
		<!-- <mapper resource="mapper/UserMapper.xml"/> -->
	</mappers>
</configuration>